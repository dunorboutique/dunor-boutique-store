---
import { resolveImageURL } from "@lib/cloudinary"

interface Props {
  src: string
  width: number
  height: number
  alt: string
  class?: string
  quality?: string
}
const { src, width, height, quality = "auto", ...props } = Astro.props as Props
const imageUrl = resolveImageURL({ src, width, height, quality })
---

<img loading="lazy" src={imageUrl} {...props} />
