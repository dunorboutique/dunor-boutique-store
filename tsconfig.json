{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@lib/*": ["./src/lib/*"],
      "@db/*": ["./src/db/*"],
      "@ui/*": ["./src/components/ui/*"],
      "@icons/*": ["./src/components/icons/*"],
      "@hooks": ["./src/hooks/index.ts"],
      "@consts": ["./src/consts.ts"],
      "@types": ["./src/types.ts"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "extends": "astro/tsconfigs/strict"
}
